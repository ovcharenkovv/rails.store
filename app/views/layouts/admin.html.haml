!!!
%html
  %head
    %title Admin panel
    = stylesheet_link_tag :all
    = javascript_include_tag :defaults
    = csrf_meta_tag
  %body
    .header.round
      .logo.round
        = link_to "Dashboard", admin_path
    .wrapper.round
      .sidebar.round
        .menu
          = render "admin/dashboard/menu"
      .content.round
        .error
          - if notice
            %p#notice= notice
            %p#alert= alert
        = yield
    .footer.round