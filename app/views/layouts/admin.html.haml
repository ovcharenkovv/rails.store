!!!
%html
  %head
    %title Admin panel
    =# stylesheet_link_tag :all
    = stylesheet_link_tag "admin"
    = javascript_include_tag :defaults
    = csrf_meta_tag
  %body
    .header
      = link_to "Dashboard", admin_path
    .wrapper
      .sidebar
        .menu
          = render "admin/dashboard/menu"
      .content
        .error
          - if notice
            %p#notice= notice
            %p#alert= alert
        = yield