!!!
%html
  %head
    %title Admin panel
    =# stylesheet_link_tag :all
    = stylesheet_link_tag "admin"
    = javascript_include_tag :defaults
    = csrf_meta_tag
    = include_tiny_mce_if_needed
  %body
    .header
      .menu
        = render "admin/dashboard/menu"
    .wrapper
      .content
        .error
          - if notice
            %p#notice= notice
            %p#alert= alert
        = yield